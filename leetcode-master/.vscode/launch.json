{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "preLaunchTask": "Build",
            "type": "cppdbg",
            "request": "launch",
            "program": "${env:windir}\\System32\\cmd.exe",
            "args": [
                "/C",
                "${workspaceFolder}/${fileBasenameNoExtension}.exe",
                "&",
                "pause"
            ],
            "externalConsole": true,
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "miDebuggerPath": "D:/Application/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "DBG",
            "preLaunchTask": "Build",
